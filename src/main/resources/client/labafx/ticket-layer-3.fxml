<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" maxHeight="150.0" maxWidth="245.0" minHeight="150.0" minWidth="245.0" prefHeight="150.0" prefWidth="245.0" style="-fx-background-color: EDEDED;" styleClass="vbox" stylesheets="@CSS/ticket.css" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.labafx.command.utility.CommandController">
    <children>
        <Label fx:id="addressLabel" text="Address">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="90.0" minWidth="240.0" prefHeight="90.0" prefWidth="240.0">
            <children>
                <VBox alignment="CENTER" minHeight="55.0" minWidth="70.0" prefHeight="55.0" prefWidth="70.0" spacing="9.0">
                    <children>
                        <Label fx:id="streetLabel" minHeight="25.0" prefHeight="25.0" text="street:">
                            <font>
                                <Font size="12.0" />
                            </font>
                        </Label>
                        <Label fx:id="zipCodeLabel" minHeight="25.0" prefHeight="25.0" text="zipCode:">
                            <font>
                                <Font size="12.0" />
                            </font>
                        </Label>
                    </children>
                    <padding>
                        <Insets bottom="10.0" left="10.0" top="10.0" />
                    </padding>
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                </VBox>
                <VBox alignment="CENTER" minHeight="40.0" minWidth="150.0" prefHeight="40.0" prefWidth="150.0">
                    <children>
                        <HBox alignment="CENTER_RIGHT" prefHeight="34.0" prefWidth="162.0" spacing="20.0">
                            <children>
                                <TextField fx:id="streetField" maxWidth="200.0" minHeight="25.0" minWidth="120.0" onKeyTyped="#setColorBlack" prefHeight="25.0" prefWidth="120.0" styleClass="rounded_node" stylesheets="@CSS/ticket.css" />
                            </children>
                            <padding>
                                <Insets right="5.0" />
                            </padding>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT" prefHeight="34.0" prefWidth="266.0" spacing="20.0">
                            <padding>
                                <Insets right="5.0" />
                            </padding>
                            <children>
                                <TextField fx:id="zipCodeField" maxWidth="200.0" minHeight="25.0" minWidth="120.0" onKeyTyped="#setColorBlack" prefHeight="25.0" prefWidth="120.0" styleClass="rounded_node" stylesheets="@CSS/ticket.css" />
                            </children>
                        </HBox>
                    </children>
                </VBox>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets left="10.0" right="10.0" top="30.0" />
    </padding>
</VBox>
